                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:text="@string/cat_toys"
                                    android:layout_marginEnd="12dp"
                                    android:textStyle="bold"
                                    android:textColor="@color/white"/>


                            </LinearLayout>



                        </androidx.cardview.widget.CardView>


